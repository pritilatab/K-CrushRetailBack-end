			//enable the search button on page load
			$("input:disabled").removeAttr("disabled");
			
			function search(q){
			
				//cleanup previous search results
				$("#prod").children().remove();
				$("#prod").hide();
				$("#search_q").remove();				
				
				let v = $.trim(q);
				
				//save the recent searches
				let d = $(this).data();
				//alert(JSON.stringify(d));
				if (d.q == null || d.q == undefined){
					$(this).data({ q: [v] });
				}else{
					let x = d.q;
					x.push(v);
				}
				
				//check if search string is blank
				if(v == "" || v == null){
					$("#class_list").fadeIn("slow");
				} else {
				
					$.getJSON( domain + "/search/"+ encodeURI(q) + "/1", function( res ) {
											
						if(res.status == 'failed') {
							let html = '<div class="nav-scroller py-1 mb-2" id="search_q">' +
										'<strong class="d-inline-block mb-2">No search results. Try another search.</strong><br></br>' +
										'</div>';
							
							$("#head_bar").after(html);
						} else {
													 
							//alert(JSON.stringify(res));
						
							for(let i=0;i<res.data.length;i++){
								url = domain + '/image/' + res.data[i].sku_Item_Number;
								
								let item = '<div class="col-md-4"> <a href="#">' +
										  '<div class="row no-gutters border rounded overflow-hidden flex-md-row mb-4 shadow-sm h-md-250 position-relative bg-white">' +
											'<div class="col p-4 d-flex flex-column position-static">' +
											  '<strong class="d-inline-block mb-2 text-success">' + res.data[i].cat_Class_Name +'</strong>' +
											  '<h5 class="mb-0">' + res.data[i].sku_Description + '</h5>' +
											  '<p class="mb-auto" >  </p>' +
											  '<p class="mb-auto" ><strong class="d-inline-block mb-2 text-success">$'+ res.data[i].prc_List_Price + '</strong></p>' +
											'</div>' +
											'<div class="col-auto d-none d-lg-block">' +
											  '<img class="bd-placeholder-img" crossorigin="anonymous"  max-width="auto" height="250" src="' + url + '" />' +
											'</div> </div> </a> </div>';
								
								$("#prod").append(item);
								//alert($("#prod").html());
							}
							
							//hide the home page contents & show the search results
							//$("#nav_carousel").hide();
							//$("#content_below").hide();
							$("#class_list").fadeOut("slow");
							
							//show the string searched for
							let html = '<div class="nav-scroller py-1 mb-2" id="search_q">' +
										'You searched for : <strong class="d-inline-block mb-2"><span>' + v + '</span></strong>. ' + 
										res.count +' items found.</div>';
							 
							$("#head_bar").after(html);
							
							//show the search results
							$("#prod").fadeIn("slow");
							
						}
					
					});
				}
				
			}
			
			$("#btn_search").on('click', function(e){
